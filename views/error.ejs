<%- layout('layouts/boilerplate') %>
<div class="row my-5">
    <div class="col-8 offset-2 my-5">
        <div class="alert alert-danger mt-5" role="alert">
            <h4 class="alert-heading"><%= err.message %></h4>
            <% if (process.env.NODE_ENV !== 'production') { %>
            <p><%= err.stack %></p>
            <% } %>
        </div>
    </div>
</div>
